{
    "name": "endroid/simple-spreadsheet",
    "description": "Endroid Simple Spreadsheet",
    "keywords": ["endroid", "simple", "spreadsheet", "php"],
    "homepage": "https://github.com/endroid/simple-spreadsheet",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": "^8.1",
        "phpoffice/phpspreadsheet": "^2.2",
        "symfony/http-foundation": "^5.4||^6.4||^7.0"
    },
    "require-dev": {
        "endroid/quality": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\SimpleSpreadsheet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\SimpleSpreadsheet\\": "tests/"
        }
    },
    "suggest": {
        "roave/security-advisories": "Avoids installation of package versions with vulnerabilities"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "endroid/installer": true
        }
    }
}
